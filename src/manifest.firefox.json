{
    "browser_specific_settings": {
        "gecko": {
            "id": "vulet@protonmail.com"
        }
    },
    "name": "Ark Dandelion",
    "description": "bip39 the interwebs",
    "version": "0.1.1",
    "manifest_version": 2,
    "icons": {
        "16": "assets/favicon-16.png",
        "32": "assets/favicon-32.png",
        "48": "assets/favicon-48.png",
        "128": "assets/favicon-128.png"
    },    
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/favicon-16.png",
            "32": "assets/favicon-32.png",
            "48": "assets/favicon-48.png",
            "128": "assets/favicon-128.png"
        },
        "default_title": "ArkDandelion",
        "browser_style": false
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "homepage_url": "https://github.com/vulet/ArkDandelion",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "short_name": "Dandelion"
}
